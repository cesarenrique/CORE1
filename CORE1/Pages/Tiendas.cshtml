

@page
@model CORE1.Pages.TiendasModel
@{
	ViewData["Title"] = "Tiendas page";
}

<h1>Tiendas</h1>

<table class="table table-striped table-bordered"
	<thead>
		<tr>
			<th>Nombre</th>
			<th>Dirección</th>
			<th>Población</th>
			<th>Email</th>
		</tr>
	</thead>
	<tbody>
		@foreach (Models.Tienda t in Model.Tiendas){
	
			<tr>

				<td>@t.nombre</td>
				<td>@t.direccion</td>
				<td>@t.localidad</td>
				<td>@t.email</td>

			</tr>
		}
	</tbody>
	
</table>